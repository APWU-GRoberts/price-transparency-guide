<?xml version="1.0" encoding="UTF-8" ?>
<_schema>http://json-schema.org/draft-07/schema#</_schema>
<definitions>
    <drugs>
        <type>object</type>
        <properties>
            <drug_name>
                <type>string</type>
            </drug_name>
            <drug_type>
                <enum>branded</enum>
                <enum>generic</enum>
                <enum>biosimilar</enum>
            </drug_type>
            <ndc>
                <type>string</type>
            </ndc>
            <prices>
                <type>array</type>
                <items>
                    <_ref>#/definitions/prices</_ref>
                </items>
            </prices>
        </properties>
        <required>drug_name</required>
        <required>drug_type</required>
        <required>ndc</required>
        <required>prices</required>
    </drugs>
    <prices>
        <type>object</type>
        <properties>
            <historical_net_price>
                <type>number</type>
            </historical_net_price>
            <historical_net_reporting_period>
                <type>string</type>
                <format>date</format>
            </historical_net_reporting_period>
            <negotiated_rate>
                <type>number</type>
            </negotiated_rate>
            <administrative_fee>
                <type>number</type>
            </administrative_fee>
            <dispensing_fee>
                <type>number</type>
            </dispensing_fee>
            <transaction_fee>
                <type>number</type>
            </transaction_fee>
            <tin>
                <type>string</type>
            </tin>
            <service_code>
                <type>string</type>
                <_ref>https://www.cms.gov/Medicare/Coding/place-of-service-codes/Place_of_Service_Code_Set</_ref>
            </service_code>
            <providers>
                <type>array</type>
                <items>
                    <_ref>#/definitions/providers</_ref>
                </items>
            </providers>
        </properties>
        <required>historical_net_price</required>
        <required>historical_net_reporting_period</required>
        <required>negotiated_rate</required>
        <required>administrative_fee</required>
        <required>dispensing_fee</required>
        <required>transaction_fee</required>
        <required>tin</required>
        <required>providers</required>
    </prices>
    <providers>
        <type>object</type>
        <properties>
            <pharmacy_id_type>
                <type>string</type>
            </pharmacy_id_type>
            <pharmacy_ids>
                <type>array</type>
                <items>
                    <type>number</type>
                </items>
                <uniqueItems>true</uniqueItems>
            </pharmacy_ids>
            <npi>
                <type>array</type>
                <items>
                    <type>number</type>
                </items>
                <uniqueItems>true</uniqueItems>
            </npi>
        </properties>
        <required>pharmacy_id_type</required>
        <required>pharmacy_ids</required>
        <required>npi</required>
    </providers>
</definitions>
<type>object</type>
<properties>
    <reporting_entity_name>
        <type>string</type>
    </reporting_entity_name>
    <reporting_entity_type>
        <type>string</type>
    </reporting_entity_type>
    <plan_name>
        <type>string</type>
    </plan_name>
    <plan_id_type>
        <type>string</type>
    </plan_id_type>
    <plan_id>
        <type>string</type>
    </plan_id>
    <plan_market_type>
        <enum>group</enum>
        <enum>individual</enum>
    </plan_market_type>
    <last_update_on>
        <type>string</type>
        <format>date</format>
    </last_update_on>
    <drugs>
        <type>array</type>
        <items>
            <_ref>#/definitions/drugs</_ref>
        </items>
    </drugs>
</properties>
<required>reporting_entity_name</required>
<required>reporting_entity_type</required>
<required>plan_name</required>
<required>plan_id_type</required>
<required>plan_id</required>
<required>plan_market_type</required>
<required>last_update_on</required>

